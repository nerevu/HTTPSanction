{
  "$schema": "../schemas/provider.schema.json",
  "prefix": "gusto",
  "auths": [
    {
      "authId": "oauth",
      "authType": "oauth2",
      "apiBaseURL": "https://api.gusto-demo.com/v1",
      "documentationURL": "https://docs.gusto.com/docs/api",
      "authorizationBaseURL": "https://api.gusto-demo.com/oauth/authorize",
      "tokenURL": "https://api.gusto-demo.com/oauth/token",
      "clientId": "$GUSTO_CLIENT_ID",
      "clientSecret": "$GUSTO_SECRET",
      "paramMap": {
        "start": "start_date",
        "end": "end_date"
      },
      "attrs": {
        "companyId": "$GUSTO_COMPANY_ID"
      },
      "scope": [
        "public"
      ]
    }
  ],
  "statusResourceId": "me",
  "resources": [
    {
      "resourceId": "companies",
      "resourcePath": "companies/{companyId}",
      "authId": "oauth"
    },
    {
      "resourceId": "employees",
      "authId": "oauth",
      "resourcePath": "companies/{companyId}/employees"
    },
    {
      "resourceId": "me",
      "authId": "oauth"
    },
    {
      "resourceId": "payments",
      "authId": "oauth",
      "resourcePath": "companies/{companyId}/contractor_payments"
    },
    {
      "resourceId": "payrolls",
      "resourcePath": "payrolls/{companyId}/contractor_payments",
      "authId": "oauth"
    }
  ]
}
