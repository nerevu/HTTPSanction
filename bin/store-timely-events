#!/usr/bin/env bash -uo pipefail

curl 'http://localhost:5000/v1/timely-time?process=true&all=true&dictify=true' | jq '.result' > app/data/timely_events.json

for PROJECT in $(jq -r 'to_entries[] | .key' app/data/timely_projects.json); do
    curl "http://localhost:5000/v1/timely-time?process=true&id=${PROJECT}" | jq '.result' > "app/data/timely_${PROJECT}_events.json"
done
