#!/usr/bin/env bash -uo pipefail

curl 'http://localhost:5000/v1/timely-users?dictify=true' | jq '.result' > app/data/timely_users.json
curl 'http://localhost:5000/v1/xero-users?dictify=true' | jq '.result' > app/data/xero_projectsusers.json
curl 'http://localhost:5000/v1/xero-contacts?dictify=true' | jq '.result' > app/data/xero_contacts.json
